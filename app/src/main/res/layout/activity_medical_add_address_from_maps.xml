<?xml version="1.0" encoding="utf-8"?>

<android.support.v4.widget.DrawerLayout
    android:id="@+id/drawer_layout"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:openDrawer="start"
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">


    <RelativeLayout android:id="@+id/relative"
        xmlns:android="http://schemas.android.com/apk/res/android"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        xmlns:ripple="http://schemas.android.com/apk/res-auto"
        android:focusable="true"
        android:focusableInTouchMode="true"
        android:focusedByDefault="false"
        android:keyboardNavigationCluster="true"
        android:orientation="vertical"
        xmlns:tools="http://schemas.android.com/tools">

        <android.support.design.widget.AppBarLayout
            android:id="@+id/appbar1"
            android:layout_width="match_parent"

            android:layout_height="wrap_content"
            android:theme="@style/AppTheme.AppBarOverlay">

            <android.support.v7.widget.Toolbar
                android:id="@+id/toolbar"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:background="@drawable/toolbar_gradient"
                app:popupTheme="@style/AppTheme.PopupOverlay"
                app:layout_scrollFlags="scroll|enterAlways" >

            </android.support.v7.widget.Toolbar>

        </android.support.design.widget.AppBarLayout>


        <ScrollView
            xmlns:android="http://schemas.android.com/apk/res/android"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:isScrollContainer="true"
            android:layout_below="@+id/appbar1"
            xmlns:tools="http://schemas.android.com/tools"
            xmlns:app="http://schemas.android.com/apk/res-auto">
            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"

                android:layout_below="@+id/appbarcool">
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="vertical"
                    android:padding="20dp">

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:weightSum="1"
                        android:layout_height="match_parent">
                        <android.support.design.widget.TextInputLayout
                            android:id="@+id/input_salutation"
                            android:layout_weight="1"
                            android:layout_width="wrap_content"
                            android:layout_marginTop="15dp"
                            android:layout_height="wrap_content"
                            android:textColorHint="#8c8080">
                            <!--<HorizontalScrollView-->
                            <!--android:layout_width="wrap_content"-->
                            <!--android:layout_height="wrap_content">-->
                            <EditText
                                android:id="@+id/Medical_Hospital_name"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:ems="20"
                                android:hint="Medical Shop Name"
                                android:textSize="15dp"
                                android:drawablePadding="5dp"
                                android:drawableLeft="@drawable/ic_account_balance_black_24dp"
                                android:inputType="textPersonName" />
                            <!--</HorizontalScrollView>-->
                        </android.support.design.widget.TextInputLayout>
                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:weightSum="1"
                        android:layout_height="match_parent">

                        <android.support.design.widget.TextInputLayout
                            android:id="@+id/address"
                            android:layout_weight="1"
                            android:layout_width="wrap_content"
                            android:layout_marginTop="15dp"
                            android:layout_height="wrap_content"
                            android:textColorHint="#8c8080"
                            android:editable="false"
                            >
                            <EditText
                                android:id="@+id/Address"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:ems="20"
                                android:textSize="15dp"
                                android:drawablePadding="5dp"
                                android:drawableLeft="@drawable/street_icons"
                                android:hint="Address"
                                android:inputType="text" />
                        </android.support.design.widget.TextInputLayout>
                    </LinearLayout>

                    <LinearLayout

                        android:layout_width="match_parent"
                        android:layout_height="match_parent">

                        <TextView
                            android:id="@+id/select_city"
                            android:textColor="#2C5E83"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="15dp"
                            android:layout_weight="1"
                            android:textStyle="bold"
                            android:textSize="16dp"
                            android:text="City :" />

                        <com.toptoche.searchablespinnerlibrary.SearchableSpinner
                            android:id="@+id/cityId"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="15dp"
                            android:backgroundTint="@color/green"
                            android:layout_weight="1" />

                    </LinearLayout>


                    <LinearLayout
                        android:layout_marginTop="15dp"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent">

                        <TextView
                            android:id="@+id/select_district"
                            android:textColor="#2C5E83"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_weight="1"
                            android:textStyle="bold"
                            android:textSize="16dp"
                            android:text="District :" />
                        <com.toptoche.searchablespinnerlibrary.SearchableSpinner
                            android:id="@+id/districtId"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_weight="1" />

                    </LinearLayout>
                    <LinearLayout
                        android:layout_marginTop="15dp"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent">

                        <TextView
                            android:id="@+id/select_state"
                            android:textColor="#2C5E83"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_weight="1"
                            android:textStyle="bold"
                            android:textSize="16dp"
                            android:text="State :" />

                        <com.toptoche.searchablespinnerlibrary.SearchableSpinner
                            android:id="@+id/stateId"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_weight="1" />

                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:weightSum="2"
                        android:layout_marginTop="15dp"
                        android:layout_height="match_parent">
                        <android.support.design.widget.TextInputLayout
                            android:id="@+id/Mobile_Numbertext"
                            android:layout_weight="1"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"

                            android:textColorHint="#8c8080"
                            android:editable="false"
                            android:layout_gravity="center">

                            <EditText
                                android:id="@+id/Mobile_Number"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:ems="10"
                                android:drawableLeft="@drawable/phone"
                                android:drawablePadding="5dp"
                                android:hint="Mobile number"
                                android:textSize="15dp"
                                android:inputType="phone" />

                        </android.support.design.widget.TextInputLayout>

                        <android.support.design.widget.TextInputLayout
                            android:layout_weight="1"
                            android:id="@+id/input_pincode"

                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"

                            android:textColorHint="#8c8080"
                            android:editable="false"
                            android:layout_gravity="center">

                            <EditText
                                android:id="@+id/pincode"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_weight="1"
                                android:ems="10"
                                android:textSize="15dp"
                                android:drawableLeft="@drawable/zipcode_icon"
                                android:drawablePadding="5dp"
                                android:hint="Pincode"
                                android:inputType="number" />

                        </android.support.design.widget.TextInputLayout>

                    </LinearLayout>

                    <LinearLayout
                        android:weightSum="2"
                        android:layout_width="match_parent"
                        android:layout_marginTop="15dp"
                        android:layout_height="match_parent">
                        <android.support.design.widget.TextInputLayout
                            android:id="@+id/Frontoffice_Contact_Person"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_weight="1"
                            android:textColorHint="#8c8080"
                            android:editable="false"
                            android:layout_gravity="center">

                            <EditText
                                android:id="@+id/Frontoffice"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:ems="10"
                                android:drawableLeft="@drawable/ic_person_black_24dp"
                                android:drawablePadding="5dp"
                                android:hint="Contact person"
                                android:textSize="15dp"
                                android:inputType="textPersonName" />

                        </android.support.design.widget.TextInputLayout>

                        <android.support.design.widget.TextInputLayout
                            android:id="@+id/Consultation_Feetext"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_weight="1"
                            android:textColorHint="#8c8080"
                            android:editable="false"
                            android:layout_gravity="center">

                            <EditText
                                android:id="@+id/landMobileNumber"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_weight="1"
                                android:ems="10"
                                android:textSize="15dp"
                                android:drawableLeft="@drawable/phone"
                                android:hint="Landline no"
                                android:inputType="number" />

                        </android.support.design.widget.TextInputLayout>

                    </LinearLayout>

                    <LinearLayout
                        android:weightSum="1"
                        android:layout_width="match_parent"
                        android:layout_marginTop="15dp"
                        android:layout_height="match_parent">
                        <android.support.design.widget.TextInputLayout
                            android:layout_weight="1"
                            android:id="@+id/Experence_id"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"

                            android:textColorHint="#8c8080"
                            android:editable="false"
                            android:layout_gravity="center">

                            <EditText
                                android:id="@+id/Experence"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:ems="20"

                                android:drawableLeft="@drawable/experience_icon"
                                android:drawablePadding="5dp"
                                android:hint="Chemist Since (Experience)"
                                android:textSize="15dp"
                                android:inputType="number" />

                        </android.support.design.widget.TextInputLayout>

                    </LinearLayout>

                    <LinearLayout
                        android:weightSum="1"
                        android:layout_marginTop="15dp"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent">

                        <TextView
                            android:id="@+id/googleLocation"
                            android:textColor="#2C5E83"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_weight="1"
                            android:textStyle="bold"
                            android:textSize="16dp"
                            android:text="@string/google_location" />

                    </LinearLayout>


                    <LinearLayout
                        android:layout_marginTop="15dp"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:orientation="horizontal">

                        <TextView
                            android:layout_width="120dp"
                            android:layout_height="wrap_content"
                            android:textSize="16sp"
                            android:layout_marginStart="4dp"
                            android:layout_weight="1"
                            android:textStyle="bold"
                            android:layout_marginBottom="5dp"
                            android:textColor="#ff0000"
                            android:text="@string/note"/>

                        <TextView
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:textSize="16sp"
                            android:layout_weight="1"
                            android:layout_marginBottom="5dp"
                            android:textColor="#ff0000"
                            android:text="@string/enter_lat_long_or_otherwise_click_below"/>
                    </LinearLayout>


                    <LinearLayout
                        android:weightSum="1"
                        android:layout_width="match_parent"

                        android:layout_height="wrap_content">

                        <TextView
                            android:id="@+id/getlatlng"
                            android:layout_weight="1"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginStart="60dp"
                            android:layout_marginEnd="60dp"
                            android:textAlignment="center"
                            android:layout_marginTop="10dp"
                            android:padding="7dp"
                            android:background="@drawable/google_map_link"
                            android:text="@string/click_here_to_get_your_location"
                            android:textColor="#fff"
                            android:textSize="15sp"
                            android:textStyle="bold" />


                    </LinearLayout>


                    <LinearLayout
                        android:weightSum="2"
                        android:layout_width="match_parent"
                        android:layout_marginTop="15dp"
                        android:layout_height="match_parent">
                        <android.support.design.widget.TextInputLayout
                            android:id="@+id/Latitudelabel"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_weight="1"
                            android:textColorHint="#8c8080"
                            android:editable="false"
                            android:layout_gravity="center">
                            <EditText
                                android:id="@+id/Latitude"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:ems="10"

                                android:hint="Latitude"
                                android:textSize="15dp"
                                android:inputType="phone" />
                        </android.support.design.widget.TextInputLayout>

                        <android.support.design.widget.TextInputLayout
                            android:id="@+id/Logitude_Label"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_weight="1"
                            android:textColorHint="#8c8080"
                            android:editable="false"
                            android:layout_gravity="center">
                            <EditText
                                android:id="@+id/Longitude"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_weight="1"
                                android:ems="10"
                                android:textSize="15dp"
                                android:hint="Longitude"
                                android:inputType="phone" />
                        </android.support.design.widget.TextInputLayout>

                    </LinearLayout>

                    <LinearLayout
                        android:weightSum="1"
                        android:layout_marginTop="15dp"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent">

                        <TextView

                            android:id="@+id/Availabilty_Timing"
                            android:textColor="#2C5E83"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_weight="1"
                            android:textStyle="bold"
                            android:textSize="16dp"
                            android:text="Availability Timing : " />

                    </LinearLayout>

                    <LinearLayout
                        android:weightSum="2"
                        android:layout_width="match_parent"
                        android:layout_marginTop="15dp"
                        android:layout_height="match_parent">
                        <android.support.design.widget.TextInputLayout
                            android:id="@+id/FromTiming"
                            android:layout_weight="1"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"

                            android:textColorHint="#8c8080"
                            android:editable="false"
                            android:layout_gravity="center">
                            <EditText
                                android:id="@+id/From"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:ems="10"
                                android:drawableRight="@drawable/ic_timer_black_24dp"
                                android:text="00:00"
                                android:hint="From Time"
                                android:textSize="15dp"
                                android:inputType="time" />
                        </android.support.design.widget.TextInputLayout>

                        <android.support.design.widget.TextInputLayout
                            android:id="@+id/To_Timinig"
                            android:layout_weight="1"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"

                            android:textColorHint="#8c8080"
                            android:editable="false"
                            android:layout_gravity="center">

                            <EditText
                                android:id="@+id/To_Timing"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_weight="1"
                                android:ems="10"
                                android:drawableRight="@drawable/ic_timer_black_24dp"
                                android:text="00.00"
                                android:textSize="15dp"
                                android:paddingRight="20dp"
                                android:hint="To Time"
                                android:inputType="time" />
                        </android.support.design.widget.TextInputLayout>

                    </LinearLayout>


                    <LinearLayout
                        android:weightSum="2"
                        android:layout_marginTop="15dp"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent">

                        <TextView
                            android:id="@+id/Comments"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_weight="0.1"

                            android:text="Comments :"
                            android:textColor="#2C5E83"
                            android:textSize="16dp"
                            android:textStyle="bold" />


                        <MultiAutoCompleteTextView

                            android:id="@+id/Comments_Others"
                            android:textColor="#000"
                            android:layout_width="wrap_content"
                            android:layout_height="50dp"
                            android:layout_marginEnd="20dp"
                            android:background="@drawable/edittext_button"
                            android:layout_weight="2"
                            android:textSize="15dp"
                            />

                    </LinearLayout>

                    <LinearLayout
                        android:weightSum="2"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent">

                        <TextView
                            android:id="@+id/Pharmacy"
                            android:textColor="#2C5E83"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="15dp"
                            android:layout_weight="1"
                            android:textStyle="bold"
                            android:textSize="16dp"
                            android:text="Pharmacy Type :" />

                        <com.toptoche.searchablespinnerlibrary.SearchableSpinner
                            android:id="@+id/Pharmacy_type"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="15dp"
                            android:backgroundTint="@color/green"
                            android:layout_weight="1" />

                    </LinearLayout>

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="Medical Store Image :"
                        android:textStyle="bold"
                        android:textColor="#2C5E83"
                        android:layout_marginTop="15dp"
                        android:textSize="16dp"/>
                    <android.support.design.widget.CoordinatorLayout

                        android:id="@+id/main"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginEnd="8dp"
                        android:layout_marginStart="8dp"
                        android:layout_marginTop="10dp"

                        >

                        <android.support.design.widget.AppBarLayout
                            android:id="@+id/appbar"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"

                            >

                            <ImageView
                                android:id="@+id/diag_center_image"
                                android:layout_width="match_parent"
                                android:layout_height="180dp"
                                android:src="@drawable/nomedicalcenter"
                                android:scaleType="fitXY"
                                />

                        </android.support.design.widget.AppBarLayout>

                        <android.support.design.widget.FloatingActionButton
                            android:id="@+id/addDiagCenterIcon"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginRight="5dp"
                            android:clickable="true"
                            android:src="@drawable/ic_camera_alt_black_24dp"
                            app:fabSize="mini"
                            app:layout_anchor="@id/appbar"
                            app:layout_anchorGravity="bottom|right|end" />

                    </android.support.design.widget.CoordinatorLayout>

                    <LinearLayout
                        android:weightSum="1"
                        android:layout_width="wrap_content"
                        android:layout_gravity="center"
                        android:layout_height="wrap_content">
                        <CheckBox

                            android:id="@+id/serviceAvailable"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginBottom="5dp"
                            android:layout_marginTop="15dp"
                            android:layout_gravity="center"
                            android:layout_weight="1"

                            android:text="Are you available for emergency."

                            android:textColor="#ff0000"
                            android:textSize="16dp"
                            android:textStyle="bold" />
                    </LinearLayout>

                    <LinearLayout
                        android:weightSum="1"
                        android:layout_width="match_parent"
                        android:id="@+id/emergencyContactLayout"
                        android:visibility="gone"
                        android:layout_marginTop="15dp"

                        android:layout_height="match_parent">
                        <android.support.design.widget.TextInputLayout
                            android:layout_weight="1"
                            android:id="@+id/emergencyContactText"
                            android:layout_width="wrap_content"
                            android:layout_marginTop="15dp"
                            android:layout_height="wrap_content"
                            android:textColorHint="#8c8080"
                            android:editable="false"
                            >
                            <EditText
                                android:id="@+id/Emergency_Contact"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:ems="20"
                                android:textSize="15dp"
                                android:drawablePadding="5dp"
                                android:drawableLeft="@drawable/phone"
                                android:hint="Emergency Contact Number"
                                android:inputType="phone" />
                        </android.support.design.widget.TextInputLayout>
                    </LinearLayout>

                    <LinearLayout
                        android:layout_gravity="center"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content">

                        <com.andexert.library.RippleView
                            android:id="@+id/rippleView"
                            android:layout_width="wrap_content"
                            android:layout_marginTop="20dp"
                            android:layout_marginBottom="20dp"
                            android:layout_gravity="center"
                            android:layout_height="wrap_content"
                            ripple:rv_type="rectangle"
                            ripple:rv_zoom="true">

                            <TextView
                                android:id="@+id/rect_child"
                                android:layout_width="150dp"
                                android:layout_height="50dp"
                                android:layout_centerInParent="true"
                                android:textColor="#fff"
                                android:textSize="18sp"
                                android:gravity="center"
                                android:text="@string/submit"
                                android:background="@color/rippleButton"/>

                        </com.andexert.library.RippleView>
                    </LinearLayout>



                </LinearLayout>
            </RelativeLayout>
        </ScrollView>

    </RelativeLayout>


    <android.support.design.widget.NavigationView
        android:id="@+id/nav_view"
        android:layout_width="wrap_content"
        android:layout_height="match_parent"
        android:layout_gravity="start"
        android:fitsSystemWindows="true">

        <RelativeLayout
            android:id="@+id/relativelayout"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content">

            <LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
                xmlns:app="http://schemas.android.com/apk/res-auto"
                android:id="@+id/layoutLinear"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="@drawable/toolbar_gradient"
                android:gravity="bottom"
                android:orientation="vertical"
                android:paddingBottom="@dimen/activity_vertical_margin"
                android:paddingLeft="@dimen/activity_horizontal_margin"
                android:paddingRight="@dimen/activity_horizontal_margin"
                android:paddingTop="@dimen/activity_vertical_margin"
                android:theme="@style/ThemeOverlay.AppCompat.Dark">

                <ImageView
                    android:id="@+id/profileImageId"
                    android:layout_width="100dp"
                    android:layout_below="@+id/spaceId"
                    android:layout_height="wrap_content"
                    android:visibility="gone"
                    android:src="@drawable/noprofile" />

                <TextView
                    android:id="@+id/name"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:textColor="@color/sidenavheading"
                    android:textStyle="bold"
                    android:textSize="20dp"
                    android:layout_marginTop="15dp"
                    android:textAlignment="center"
                    android:drawablePadding="5dp"
                    android:text="Vijay UN"
                    android:textAppearance="@style/TextAppearance.AppCompat.Body1" />

                <TextView
                    android:id="@+id/mobile"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textSize="15dp"
                    android:textColor="@color/sidenavtext"
                    android:layout_marginTop="6dp"
                    android:drawableLeft="@drawable/sidenavphoneicon"
                    android:drawablePadding="5dp"
                    android:text="9676856122"/>

                <TextView
                    android:layout_marginTop="6dp"
                    android:id="@+id/emailId"
                    android:textSize="15dp"
                    android:textColor="@color/sidenavtext"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
